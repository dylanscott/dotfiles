snippet ex "expect" b
expect(${1:${VISUAL}})${0}
endsnippet

snippet describe "describe" b
describe('${1}', () => {
  ${0:${VISUAL}}
})
endsnippet

snippet it "it" b
it('${1}', () => {
  ${0:${VISUAL:expect.fail()}}
})
endsnippet

snippet ita "it (async)" b
it('${1}', (done) => {
  ${0:${VISUAL:expect.fail()}}
  done()
})
endsnippet

